
@{
    ViewBag.Title = "XOVO";
}

<div class="card hoverable center">
    <span class="card-title center">Beitrag verfassen</span>
    <div class="row">
        <form class="col s12">
            <div class="card-content row">
                <div class="input-field col s12">
                    <i class="material-icons prefix">mode_edit</i>
                    <textarea id="icon_prefix2" class="materialize-textarea"></textarea>
                    <label for="icon_prefix2">Beitrag</label>
                </div>
            </div>
            <div class="row">
                <form action="#">
                    <div class="file-field input-field col s5">
                        <div class="btn cyan darken-2">
                            <span>Datei</span>
                            <input type="file" multiple>
                        </div>
                        <div class="file-path-wrapper">
                            <input class="file-path validate" type="text" placeholder="Eine oder mehrere Dateien hochladen">
                        </div>
                    </div>
                </form>
                <div class="card-action">
                    <button class="btn waves-effect waves-light" type="submit" name="action">
                        Beitrag teilen
                        <i class="material-icons right">send</i>
                    </button>
                </div>
            </div>
        </form>
    </div>
</div>

